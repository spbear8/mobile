page {
    height: 100%;
    background-color: #eee;
    font-family: "苹方";
    color: #333;
}

.swiper {
    height: 350rpx;
    width: 100%;
    position: relative;
}

.swiper image {
    height: 100%;
    width: 100%;
}

.header {
    height: 47%;
    box-sizing: border-box;
    padding-top: 100rpx;
    background-color: #FFDA47;
    text-align: center;
}

.icon-wrapper {
    width: 200rpx;
    height: 200rpx;
    border: 4rpx solid #fff;
    margin: 0 auto;
    margin-bottom: 16rpx;
    border-radius: 50%;
    overflow: hidden;
    text-align: center;
}

.icon-inner {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    border: 8rpx solid #FFDA47;
    border-radius: 50%;
    background-color: #fff;
    overflow: hidden;
    box-sizing: border-box;
}

.tel-img {
    width: 134rpx;
    height: 134rpx;
}

.header-title {
    font-size: 34rpx;
}

.changable {
    width: 170rpx;
    padding: 10rpx 0;
    background-color: #FF9300;
    text-align: center;
    margin: 0 auto;
    margin-top: 30rpx;
    color: #fff;
    border-radius: 30rpx;
    font-size: 26rpx;
}

.main {
    position: relative;
    top: -8vw;
    margin: 0 auto;
    display: flex;
    padding: 0;
    width: 92%;
    border-radius: 6rpx;
    background-color: #fff;
    align-items: stretch;
    justify-content: flex-start;
    flex-wrap: wrap;
    margin-top: -1px;
}

.hour_img {
    text-align: center;
    margin-bottom: 120rpx;
}

.hour_img image {
    width: 65vw;
    height: 90rpx;
}

.fix-block {
    position: relative;
    width: 33.3%;
    line-height: 120rpx;
    border: 1px solid #e3e3e3;
    text-align: center;
    background-color: #fff;
    border-left: 1px solid #fff;
    border-top: 1px solid #fff;
    box-sizing: border-box;
}

.fix-block:nth-of-type(3n) {
    border-right: 1px solid #fff;
}

.fix-block.choose {
    border-color: #15afff;
    margin-bottom: 0;
    margin-right: 0;
    z-index: 99;
    border-bottom: 1px solid #15afff;
    border-right: 1px solid #15afff;
}

.gou {
    display: inline-block;
    width: 0px;
    height: 0px;
    border-bottom: 20px solid #fff;
    border-left: 20px solid transparent;
    position: absolute;
    bottom: 0;
    right: 0;
    font-style: normal;
}

.gou::before {
    content: "";
    margin: 10px 0 0 -10px;
    position: absolute;
    width: 7px;
    height: 3px;
    border-left: 2px solid #fff;
    border-bottom: 2px solid #fff;
    transform: rotate(-45deg);
}

.gou.choose {
    border-bottom-color: #15AFFF;
}

.fix-item-img {
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
    margin-right: 8rpx;
}

.fix-item-content {
    font-size: 26rpx;
    white-space: nowrap;
}

.check-icon {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    bottom: 0;
    right: 0;
}

.footer {
    width: 100%;
    position: fixed;
    bottom: 0;
}

.footer-panel {
    height: 110rpx;
    display: flex;
    background-color: #fff;
}

.flex-container {
    flex-basis: 68%;
    height: 100%;
}

.img-field {
    flex-basis: 22%;
}

.img-wrapper {
    width: 120rpx;
    height: 135rpx;
    border-radius: 50%;
    margin-left: 20rpx;
    position: relative;
    top: -30rpx;
    text-align: center;
}

.phone-img {
    position: relative;
    width: 110rpx;
    height: 110rpx;
    top: 4.5vw;
}

.phone-img-hl {
    width: 110rpx;
    height: 110rpx;
    margin-left: 20rpx;
    position: relative;
}

.price-field {
    flex-basis: 48%;
    padding-left: 20rpx;
}

.left-field {
    width: 30%;
    height: 100%;
    position: relative;
}

.right-field {
    width: 67%;
    height: 100%;
    line-height: 44rpx;
    float: right;
    padding-left: 20rpx;
}

.text-lg {
    line-height: 120rpx;
}

.price-info {
    font-size: 32rpx;
    color: grey;
}

.price {
    display: block;
    font-size: 36rpx;
    color: red;
    padding-top: 10rpx;
}

.price-msg {
    font-size: 24rpx;
    color: grey;
    position: relative;
    top: -6rpx;
}

.base-btn {
    flex-basis: 32%;
    height: 76%;
    top: 12%;
    right: 2%;
    line-height: 90rpx;
    border-radius: 0;
    color: #fff;
    font-size: 32rpx;
    border: none;
}

.base-btn::after {
    border: none;
}

.grey-btn {
    background-color: grey;
}

.order-btn {
    background-color: #15afff;
}

.footer-modal {
    width: 100%;
    position: absolute;
    bottom: 120rpx;
}

.footer-modal-title {
    padding: 20rpx;
    padding-bottom: 40rpx;
    font-size: 34rpx;
    background-color: #F9F9F9;
    border-bottom: 1px solid gainsboro;
}

.footer-modal-list {
    background-color: #fff;
    font-size: 28rpx;
    color: grey;
    padding: 0rpx 20rpx;
}

.footer-modal-item {
    display: flex;
    padding: 20rpx;
    border-bottom: 1px solid gainsboro;
}

.first-column {
    flex-basis: 70%;
}

.second-column {
    flex-basis: 20%;
    text-align: right;
}

.third-column {
    flex-basis: 10%;
}

.cancel-field {
    width: 36rpx;
    height: 36rpx;
    border: 2rpx solid gainsboro;
    border-radius: 50%;
    line-height: 32rpx;
    font-size: 34rpx;
    text-align: center;
    float: right;
}

.angle-icon {
    width: 40rpx;
    height: 40rpx;
    position: relative;
    left: 50rpx;
    top: 22rpx;
}

.modal-phone-wrapper {
    width: 130rpx;
    height: 130rpx;
    box-sizing: border-box;
    padding-top: 10rpx;
    padding-right: 10rpx;
    position: absolute;
    left: 12rpx;
    top: 0rpx;
}

.modal-phone-img {
    width: 100rpx;
    height: 100rpx;
}

.fix-list {
    overflow: hidden;
}

.fix-item {
    padding: 20rpx;
    font-size: 26rpx;
    border: 1px solid gainsboro;
    border-radius: 8rpx;
    margin-bottom: 14rpx;
    text-align: left;
}

.fix-item-selected {
    background-color: rgba(21,175,255,0.5);
    border-color: transparent;
    color: #fff;
}

.pull-right {
    float: right;
}

.modal-container {
    height: auto;
}

.modal-btn-field {
    padding-top: 30rpx;
    overflow: hidden;
}

.modal-foot-btn {
    width: 48%;
    height: 90rpx;
    font-size: 34rpx;
}

.cancel-btn {
    color: grey;
    background-color: #fff;
    border: 1px solid gainsboro;
    float: left;
    letter-spacing: 20rpx;
    text-indent: 20rpx;
    text-align: center;
}

.cancel-btn::after {
    border: none;
}

.ok-btn {
    background-color: #15afff;
    border-color: #15afff;
    float: right;
    color: #fff;
    letter-spacing: 20rpx;
    text-indent: 20rpx;
    text-align: center;
}

.user-field {
    width: 110rpx;
    height: 11vw;
    background-color: #fff;
    border-top-left-radius: 45%;
    border-bottom-left-radius: 45%;
    position: absolute;
    right: 0;
    top: 4vw;
    overflow: hidden;
    padding: 5rpx;
    box-sizing: border-box;
}

.user-img {
    width: 72rpx;
    height: 72rpx;
    border-radius: 50%;
    position: relative;
    top: 2rpx;
}

.picker-container {
    width: 630rpx;
    position: absolute;
    left: calc(50% - 315rpx);
    top: calc(48.8% - 310rpx);
    background-color: #fff;
    border-radius: 20rpx;
    text-align: center;
    padding: 5vw 20rpx;
    z-index: 1000;
    box-sizing: border-box;
}

.picker {
    width: 100%;
    height: 460rpx;
    font-size: 30rpx;
}

picker-view-column {
    flex: none;
}

.block-btn {
    width: 95%;
    background-color: #15afff;
    color: #fff;
    letter-spacing: 20rpx;
    text-indent: 20rpx;
    text-align: center;
}

.num-tips {
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    color: #fff;
    background-color: #ff1631;
    border-radius: 50%;
    position: absolute;
    top: 6rpx;
    right: 14rpx;
    font-size: 24rpx;
}

.nav,.deafult_list {
    position: relative;
    top: -10vw;
    margin: 0 auto;
    padding: 0 5%;
    width: 82%;
    background-color: #fff;
}

.nav image {
    width: 100rpx;
    height: 100rpx;
}

.phone_pic image {
    width: 90rpx;
    height: 90rpx;
    margin-left: -1vw;
}

.nav_hd {
    padding: 4% 0;
    width: 100%;
    height: 90rpx;
    border-bottom: 1px solid #e3e3e3;
}

.nav_bd {
    padding: 4% 0;
    width: 100%;
    height: 90rpx;
}

.nav_l {
    width: 15%;
    float: left;
}

.nav_cont {
    width: 61%;
    float: left;
    line-height: 45rpx;
}

.nav_r {
    position: relative;
    top: 2vw;
    width: 24%;
    float: left;
}

.nav_cont .cont_hd_txt,.jx_txt {
    font-size: 32rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.nav_cont .cont_bd_txt {
    display: block;
    font-size: 24rpx;
    color: #999;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cont_hd_num {
    color: #ff1631;
    font-size: 24rpx;
}

.nav_r text {
    background-color: #fff;
    font-size: 28rpx;
}

.nav_r image {
    position: relative;
    margin-left: 2vw;
    width: 18rpx;
    height: 25rpx;
}

.sharebutton {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    position: fixed;
    right: 20rpx;
    bottom: 140rpx;
    background-size: 45%;
}

.kfbutton {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    position: fixed;
    right: 20rpx;
    bottom: 250rpx;
    background-size: 45%;
}

.sharebutton image {
    width: 55rpx;
    height: 55rpx;
    margin-top: 22rpx;
    margin-left: -2rpx;
}

.kfbutton image {
    width: 55rpx;
    height: 55rpx;
    margin-top: 22rpx;
    margin-left: -2rpx;
}